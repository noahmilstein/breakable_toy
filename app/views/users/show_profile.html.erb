<div class="">
  Name: <%= @user.first_name %> <%= @user.last_name %></br >
  Username: <%= @user.username %></br >
  Email: <%= @user.email %></br >
  Phone: <%= @user.phone %></br >
  Location: <%= @user.city %>,
  <%= @user.state %>,
  <%= @user.country %></br >
  <%= @user.zip %>
  <%= button_to "Request Admin Status", request_admin_users_path %>


  <% if @user.id == current_user.id %>
    <%= link_to("Edit Profile", edit_user_registration_path) %>
  <% end %>
</div>
