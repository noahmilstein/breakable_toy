<%= will_paginate @coaches %>

<div class="">
  <% @coaches.each do |coach| %>
    <div class="row callout">
      <%= coach.username + ', SSC'%></br >
      <%= coach.city %>,
      <%= coach.state %>,
      <%= coach.country %></br >
      <%= link_to "Send Email", "mailto:#{coach.email}" %></br >
      <button>Send Text</button>
    </div>
  <% end %>
</div>
